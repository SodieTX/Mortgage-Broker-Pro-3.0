{
  "name": "emc2-core",
  "version": "0.0.1",
  "description": "Core workflow orchestration service - Simple and maintainable",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/index.js",
    "check": "node scripts/check-dependencies.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:world-class": "ts-node src/testing/runWorldClassTests.ts",
    "test:chaos": "ts-node src/testing/runWorldClassTests.ts chaos",
    "test:mutation": "ts-node src/testing/runWorldClassTests.ts mutation",
    "test:time-travel": "ts-node src/testing/runWorldClassTests.ts time-travel",
    "test:sleep-well": "ts-node src/testing/runWorldClassTests.ts sleep-well",
    "test:all": "ts-node src/testing/runWorldClassTests.ts all",
    "test:coverage:report": "jest --coverage && open coverage/lcov-report/index.html",
    "test:coverage:ci": "jest --coverage --ci --maxWorkers=2",
    "test:unit": "jest --testPathIgnorePatterns=integration",
    "test:integration": "jest --testPathPattern=integration",
    "test:verbose": "jest --verbose --detectOpenHandles",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "config": "ts-node src/cli/config-cli.ts",
    "db:setup": "node scripts/setup-database.js",
    "db:setup:prod": "NODE_ENV=production node scripts/setup-database.js",
    "db:migrate": "echo 'Database migrations not implemented yet'",
    "db:backup": "docker exec mortgage_broker_db pg_dump -U mortgage_user mortgage_broker_pro > backup-$(date +%Y%m%d-%H%M%S).sql",
    "observability": "node scripts/observability.js",
    "observability:start": "node scripts/observability.js start",
    "observability:stop": "node scripts/observability.js stop",
    "observability:status": "node scripts/observability.js status",
    "health:check": "curl -f http://localhost:3001/health || exit 1",
    "security:audit": "npm audit --audit-level moderate",
    "docker:build": "docker build -t emc2-core:latest .",
    "docker:run": "docker run -d --name emc2-core -p 3001:3001 --env-file .env.production emc2-core:latest",
    "pm2:start": "pm2 start dist/index.js --name emc2-core",
    "pm2:stop": "pm2 stop emc2-core",
    "pm2:restart": "pm2 restart emc2-core",
    "pm2:logs": "pm2 logs emc2-core"
  },
  "keywords": [
    "mortgage",
    "workflow",
    "simple"
  ],
  "author": "Mortgage Broker Pro",
  "license": "PRIVATE",
  "dependencies": {
    "@aws-sdk/client-ses": "^3.840.0",
    "@azure/identity": "^4.10.2",
    "@azure/keyvault-secrets": "^4.10.0",
    "@azure/storage-blob": "^12.17.0",
    "@fastify/cors": "^8.5.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^7.2.4",
    "@fastify/multipart": "^8.3.1",
    "@fastify/rate-limit": "^8.1.1",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.60.1",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.202.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.202.0",
    "@opentelemetry/instrumentation-fastify": "^0.47.0",
    "@opentelemetry/instrumentation-http": "^0.202.0",
    "@opentelemetry/instrumentation-ioredis": "^0.50.0",
    "@opentelemetry/instrumentation-pg": "^0.54.0",
    "@opentelemetry/instrumentation-redis-4": "^0.49.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-metrics": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.202.0",
    "@opentelemetry/sdk-trace-node": "^2.0.1",
    "@opentelemetry/semantic-conventions": "^1.34.0",
    "@sendgrid/mail": "^8.1.5",
    "@types/bcrypt": "^5.0.2",
    "@types/bull": "^3.15.9",
    "@types/form-data": "^2.2.1",
    "@types/handlebars": "^4.0.40",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^6.4.17",
